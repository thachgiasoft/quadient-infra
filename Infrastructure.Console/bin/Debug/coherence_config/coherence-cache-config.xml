<?xml version="1.0"?>
<cache-config xmlns="http://schemas.tangosol.com/cache">
  <caching-scheme-mapping>
    <cache-mapping>
      <cache-name>dist-extend</cache-name>
      <scheme-name>dist-default</scheme-name>
    </cache-mapping>
  </caching-scheme-mapping>
  <caching-schemes>
    <remote-cache-scheme>
      <scheme-name>dist-default</scheme-name>
      <service-name>ExtendTcpProxyService</service-name>
      <initiator-config>
        <tcp-initiator>
          <remote-addresses>
            <socket-address>
              <address>10.0.20.211</address>
              <port>7574</port>
            </socket-address>
            <socket-address>
              <address>10.0.20.212</address>
              <port>7574</port>
            </socket-address>
          </remote-addresses>
        </tcp-initiator>
        <outgoing-message-handler>
          <heartbeat-interval>60s</heartbeat-interval>
          <heartbeat-timeout>240s</heartbeat-timeout>
        </outgoing-message-handler>
        <!--<serializer>
          <class-name>Tangosol.IO.Pof.ConfigurablePofContext, Coherence</class-name>
          <init-params>
            <init-param>
              <param-type>string</param-type>
              <param-value>config/pof-config.xml</param-value>
            </init-param>
          </init-params>
        </serializer>-->
      </initiator-config>
    </remote-cache-scheme>
  </caching-schemes>
</cache-config>
